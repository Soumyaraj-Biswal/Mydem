<%- include("partials/header") %>
<section class="section wb">
    <div class="container">
        <div class="row">
            <div class="col-lg-10 offset-lg-1">
                <div class="row">
                    <div class="col-lg-8 offset-lg-2">
                        <form class="form-wrapper" action="/register" method="post">

                        
                            <input class="form-control" type="text" name="username" placeholder="username">
                        
                            <input class="form-control" type="password" name="password" placeholder="password">
                        
                            <input class="form-control" type="text" name="avatar" placeholder="Avatar img url">
                        
                            <input class="form-control" type="text" name="firstName" placeholder="First-Name">
                        
                            <input class="form-control" type="text" name="lastName" placeholder="Last-Name">
                       
                            <input class="form-control" type="email" name="email" placeholder="john@doe.com">
                        
                            <input class="form-control" type="text" name="admin" placeholder="admincode">
                        
                            <button class="btn btn-primary btn-block" type="submit">submit</button>
                        </div>
                        
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>

<%- include("partials/footer") %>